{
  "compilerOptions": {
      "baseUrl": ".",
      "declaration": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "inlineSources": false,
      "lib": [
          "es2015",
          "dom"
      ],
      "module": "es2015",
      "moduleResolution": "node",
      "rootDir": ".",
      "skipLibCheck": true,
      "sourceMap": false,
      "strict": false,
      "strictNullChecks": true,
      "target": "es5",
      "typeRoots": [
          "../node_modules/@types"
      ],
      "types": [
          "node"
      ]
  }
}